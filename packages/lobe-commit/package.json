{
  "name": "@lobehub/commit-cli",
  "version": "2.10.0",
  "description": "Lobe Commit is a CLI tool that uses ChatGPT to generate Gitmoji-based commit messages",
  "keywords": [
    "ai",
    "git",
    "commit",
    "openai",
    "gpt",
    "gitmoji-cli",
    "git-commits",
    "chatgpt",
    "aicommit",
    "ai-commit"
  ],
  "homepage": "https://github.comlobehub/lobe-cli-toolbox/tree/master/packages/lobe-commit",
  "bugs": {
    "url": "https://github.com/lobehub/lobe-cli-toolbox/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lobehub/lobe-cli-toolbox.git"
  },
  "license": "MIT",
  "author": "canisminor1990 <i@canisminor.cc>",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "@": "./src"
  },
  "bin": {
    "lobe": "dist/cli.js",
    "lobe-commit": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run type-check && pkgroll --minify -p tsconfig.prod.json --env.NODE_ENV=production && npm run shebang",
    "dev": "pkgroll -p tsconfig.prod.json --env.NODE_ENV=development --watch",
    "link": "npm run build && npm link -f",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "shebang": "node ./dist/cli.js -t ./dist/cli.js",
    "start": "node ./dist/cli.js",
    "test": "npm run type-check",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@inkjs/ui": "^1",
    "@lobehub/cli-ui": "1.9.0",
    "chalk": "^5",
    "commander": "^11",
    "conf": "^11",
    "execa": "^7",
    "fast-deep-equal": "^3",
    "gitconfig": "^2",
    "ink": "^4.2",
    "langchain": "latest",
    "lodash-es": "^4",
    "octokit": "^3",
    "pangu": "^4",
    "path-exists": "^5",
    "react": "^18",
    "update-notifier": "^6",
    "zustand": "^4"
  },
  "peerDependencies": {
    "ink": ">=4",
    "react": ">=18"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
